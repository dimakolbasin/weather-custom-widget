{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "noImplicitAny": false,
    "lib": ["es6", "dom"],
    "types": ["node"],
    "target": "es5",
    "allowJs": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  }
}
